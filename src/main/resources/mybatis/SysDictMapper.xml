        <result column="modify_by" property="modifyBy" />
        <result column="modify_time" property="modifyTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, parent_id, layers, code, name, description, sort_num, enable_delete, create_by, create_time, modify_by, modify_time
