<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.litu.app.dao.SysAccesstokenMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.litu.app.entity.SysAccesstoken">
        <id column="f_id" property="fId"/>
        <result column="f_clientType" property="fClienttype"/>
        <result column="f_userId" property="fUserid"/>
        <result column="f_token" property="fToken"/>
        <result column="f_refreshToken" property="fRefreshtoken"/>
        <result column="f_clientMCode" property="fClientmcode"/>
        <result column="f_clientIp" property="fClientip"/>
        <result column="f_clientMac" property="fClientmac"/>
        <result column="f_createTime" property="fCreatetime"/>
        <result column="f_modifyTime" property="fModifytime"/>
        <result column="f_enableTime" property="fEnabletime"/>
        <result column="f_enableRefreshTime" property="fEnablerefreshtime"/>
        <result column="f_enabledFlag" property="fEnabledflag"/>
        <result column="f_remark" property="fRemark"/>
        <result column="f_field1" property="fField1"/>
        <result column="f_field2" property="fField2"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        f_id, f_clientType, f_userId, f_token, f_refreshToken, f_clientMCode, f_clientIp, f_clientMac, f_createTime, f_modifyTime, f_enableTime, f_enableRefreshTime, f_enabledFlag, f_remark, f_field1, f_field2
    </sql>

    <select id="selectPage" parameterType="java.lang.String" resultMap="BaseResultMap">
        SELECT
        sat.f_id,
        su.f_account as f_userId,
        su.f_realName as f_field2,
        sat.f_clientMCode,
        sat.f_clientIp,
        sat.f_clientMac,
        sat.f_enabledFlag
        FROM Sys_AccessToken sat
        LEFT JOIN Sys_User su ON sat.f_userId=su.f_id
        <where>
            ${ew.sqlSegment}
        </where>
    </select>
</mapper>
